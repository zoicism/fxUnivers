<script>
$(document).ready(function() {
    $('.userpic').hover(function() {
        $('.userpic').html('CHANGE');
    }, function() {
        $('.userpic').html('');
    });
});
</script>

<script>
$(document).ready(function() {
    $('#ads1').hover(function() {
        $('#ads1').css({opacity:1});
    }, function() {
        $('#ads1').css({opacity:0.8});
    });
});
</script>

