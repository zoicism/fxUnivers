<?php
require('../register/connect.php');
if(isset($_POST['notif_userId'])) $user_id=$_POST['notif_userId'];
$notif_icon_query="SELECT active FROM notif WHERE user_id=$user_id ORDER BY id DESC";
$notif_icon_result=mysqli_query($connection,$notif_icon_query) or die(mysqli_error($connection));
$notif_icon_fetch=mysqli_fetch_array($notif_icon_result);
$notif_last_active=$notif_icon_fetch['active'];
echo $notif_last_active;
?>